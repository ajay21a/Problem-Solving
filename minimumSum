class Solution {
    public int minimumSum(int num) {
        int arr[]=new int[4];
        int l=4,i=0;
        while(l-->0){
            arr[i++]=num%10;
            num/=10;
        }
        Arrays.sort(arr);
        int a=arr[0]*10+arr[3];
        int b=arr[1]*10+arr[2];
           int c=a+b;
    return c;
    }
 
}
